        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfVideo89"
        mc:Ignorable="d"
        Title="Actualizar" Height="120" Width="280" Closed="Window_Closed" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Icon="/refresh.png">
    <Grid>
        <TextBox x:Name="textBoxNombre" HorizontalAlignment="Left" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="165"/>
        <Label Content="Nombre del cliente" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnActualizar" Content="Actualizar" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="193,41,0,0" RenderTransformOrigin="2.733,-0.889" Click="btnActualizar_Click"/>

    </Grid>
</Window>
